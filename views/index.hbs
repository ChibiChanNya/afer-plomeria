<section class="contact-box">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-md-offset-1 col-md-10 card ">
                <div class="row m-v-0">
                    <div class="col-xs-12 col-md-8 contact-form display-flex">
                        <h3 class="margin-v">Send us your message</h3>
                        <h3 class="submitted" style="display: none;">Thanks for getting in touch.</h3>
                        <form method="POST" action="/">
                            <input type="hidden" name="action" value="contact-form/send">
                            <div class="row m-v-0"><div class="col-xs-12 col-md-5">
                                <label for="fromName">Your Name</label>
                                <input id="name" type="text" name="fromName" required="required"></div>
                                <div class="col-xs-12 col-md-5 col-md-offset-2">
                                    <label for="fromEmail">E-Mail</label>
                                    <input id="email" type="email" name="fromEmail" required="required"></div>
                                <div class="col-xs-12 col-md-5"><label for="message[Phone]">Phone</label>
                                    <input id="phone" type="text" name="message[Phone]" required="required"></div>
                                <div class="col-xs-12 col-md-5 col-md-offset-2"><label for="message[Company]">Company</label>
                                    <input id="company" type="text" name="message[Company]" required="required"></div>
                                <div class="col-xs-12 col-md-9"><label for="message[body]">Message</label>
                                    <textarea id="message" name="message[body]" required="required"></textarea>
                                </div> <div class="col-xs-12 col-md-3 display-flex">
                                    <button type="submit" class="btn btn-primary-square m-b-25 m-t-auto">
                                        <span>Send</span><span style="display: none;">
                                        <i class="fa fa-cog fa-spin"></i>
                                    </span>
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="col-xs-12 col-md-4 contact-information">
                        <h3>Contact Information</h3>
                        <p class="p-l-30 m-t-0 m-b-10">
                            <i class="fa fa-absolute fa-map-marker"></i>
                            15/1, Caledonia Tower, Qui-si-sana, Sliema, SLM3114, Malta
                        </p>
                        <p class="p-l-30 m-t-0 m-b-10">
                            <i class="fa fa-absolute fa-envelope-o"></i>
                            info@webveto.com</p>
                        <p class="m-b-20 m-t-auto">
                        </p>
                    </div>
                </div>
            </div>
        </div>
