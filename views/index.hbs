<script>
    <!-- CODE TO RUN TO SWAP NAVBAR -->
    let nav_trans = true;

    $(document).ready(function () {
        // Handler for .ready() called.
        let element_position = $('#scroll-to').offset().top;
        let screen_height = $(window).height();
        let activation_offset = 0.3;//determines how far up the the page the element needs to be before triggering the function
        let activation_point = element_position - (screen_height * activation_offset);
        let max_scroll_height = $('body').height() - screen_height - 5;//-5 for a little bit of buffer
        //Does something when user scrolls to it OR
        //Does it when user has reached the bottom of the page and hasn't triggered the function yet
        $(window).on('scroll', function () {
            let y_scroll_pos = window.pageYOffset;
            let element_in_view = y_scroll_pos > activation_point;
            let has_reached_bottom_of_page = max_scroll_height <= y_scroll_pos && !element_in_view;

            if (nav_trans && (element_in_view || has_reached_bottom_of_page)) {
                swapNav();
            }
            else if (!nav_trans && !element_in_view && !has_reached_bottom_of_page) {
                swapNav();
            }
        });

        function swapNav() {
            if (nav_trans) {
                $('#navbar').addClass('sticky-top').addClass('scrolled').animate({opacity: 1}, 300);
                nav_trans = false;
            }
            else {
                $('#navbar').animate({opacity: 0}, 300, function () {
                    $(this).removeClass('sticky-top').removeClass('scrolled').css({opacity: 1});
                });
                nav_trans = true;
            }
        }

        //    SMOOTH SCROLLING
        // need different offset on mobile
        let offset = -80;
        if ($(window).width() < 992) {
            offset = -360;
        }
        // Functionality to show-hide sections when type is picked
        function hideAll(){
            $('.hideable').hide();
        }

        $('#plomero-btn').on('click', function(){
            hideAll();
            $('.plomero-only').show();
        });

        $('#constructor-btn').on('click', function(){
            hideAll();
            $('.constructora-only').show();
        });

        $('#client-btn').on('click', function(){
            hideAll();
            $('.mostrador-only').show();
        });

        $('#gobierno-btn').on('click', function(){
            hideAll();
            $('.gobierno-only').show();
        });

        $('a.scroll[href*="#"]:not([href="#"])').click(function () {
            if (location.pathname.replace(/^\//, '') == this.pathname.replace(/^\//, '') && location.hostname == this.hostname) {
                let target = $(this.hash);
                target = target.length ? target : $('[name=' + this.hash.slice(1) + ']');
                if (target.length) {
                    $('html, body').animate({
                        scrollTop: target.offset().top + offset
                    }, 1000);
                    return false;
                }
            }
        });

        // Mobile Nav auto-collapse
        $('.navbar a.nav-link').on('click', function () {
            if( $('.navbar-collapse').hasClass('show'))
                $('.navbar-toggler').click(); //bootstrap 4.x by Richard
        });

        //   Client Button Control Carousel
        $('#client-btn').click(function () {
            $('#client-slide').click();
        });

        //    Adjust INICIO to be same size as Video-Wrapper on view
        let wrapper_height = $('.video-wrapper').height();
        $('#inicio').height(wrapper_height - 180);

        //GO to proper form when cotizacion-btn is pressed
        $('#cotizacion-btn').on('click', function(){
            $('#cotizacion-tab').click();
        });


        // Initialize Swiper Js
        let swiper = new Swiper('.swiper-container', {
            slidesPerView: 3,
            spaceBetween: 10,
            slidesPerGroup: 3,
            loop: true,
            loopFillGroupWithBlank: true,
            pagination: {
                el: '.swiper-pagination',
                clickable: true,
            },
            navigation: {
                nextEl: '.swiper-button-next',
                prevEl: '.swiper-button-prev',
            },
        });


        // Handle Contact Form
        $('#contact-form').submit(function(){
            $('#contact-submit').prop( "disabled", true );
            $.post( "/contact-mail", $( "#contact-form" ).serialize(), function(data){
                if(data.mail){
                    $('#contact-form .row').append('<p class="blue">Tu mensaje ha sido enviado. Gracias por comunicarte con nosotros.</p>');
                    console.log("SENT!");
                    $('#contact-submit').prop( "disabled", true );
                }
            });
            return false;
        });

        $('#cotizacion-form').submit(function(){
            $('#cotizacion-submit').prop( "disabled", true );
            $.post( "/products-mail", $( "#cotizacion-form" ).serialize(), function(data){
                if(data.mail){
                    $('#cotizacion-form .row').append('<p class="blue">Tu mensaje ha sido enviado. Gracias por comunicarte con nosotros.</p>');
                    console.log("SENT!");
                    $('#cotizacion-submit').prop( "disabled", false );
                }
            });
            return false;
        })

    });


</script>
